$(LLVM_HOME)/include/llvm-c/BitWriter.h
$(LLVM_HOME)/include/llvm-c/Initialization.h
$(LLVM_HOME)/include/llvm-c/Object.h
$(LLVM_HOME)/include/llvm-c/Target.h
