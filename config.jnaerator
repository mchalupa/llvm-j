-library LLVM
-runtime JNA
-direct
-limitComments
-f  // force overwrite
-emptyStructsAsForwardDecls
-mode Directory
-package org.sosy_lab.llvm_j.binding
-o $(DIR)/src
-I$(LLVM_HOME)

$(LLVM_HOME)/Transforms/*.h
$(LLVM_HOME)/Core.h
$(LLVM_HOME)/Analysis.h
$(LLVM_HOME)/BitWriter.h
$(LLVM_HOME)/BitReader.h
$(LLVM_HOME)/IRReader.h
$(LLVM_HOME)/Initialization.h
$(LLVM_HOME)/Object.h
$(LLVM_HOME)/Target.h

